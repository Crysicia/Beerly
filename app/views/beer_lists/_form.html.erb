<%= form_for @beerlist, remote: true do |f| %>
  <%= f.label :beer_id %>
  <%= f.number_field :beer_id %> </br>
  <%= f.label :bar_id %>
  <%= f.number_field :bar_id %> </br>
  <%= f.label :pint_price %>
  <%= f.number_field :pint_price %> </br>
  <%= f.label :half_pint_price %>
  <%= f.number_field :half_pint_price %>
  <%= f.submit %>
<% end %>